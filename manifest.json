{
  "manifest_version": 3,
  "name": "Project Marker",
  "version": "1.0",
  "description": "Custom New Tab",
  "permissions": ["tabs", "bookmarks"],
  "chrome_url_overrides": {
    "newtab": "newtab-vue/dist/index.html"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
